{
  "root": true,
  "plugins": [
    "jane"
  ],
  "extends": [
    "plugin:jane/recommended",
    "plugin:jane/typescript",
    "plugin:jane/react",
    "plugin:jane/prettier-ts"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "env": {
    "browser": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "jsx-a11y/anchor-is-valid": 0
  }
}
